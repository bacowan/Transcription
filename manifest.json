{
    "name": "Transcription",
    "version": "0.1",
    "description": "Chrome browser plugin for crowdsourced subtitles",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png",
          "128": "images/get_started128.png"
        }
    },
    "background": {
        "scripts": [
            "backgroundScripts/get_transcription.js",
            "backgroundScripts/message_listener.js"
        ],
        "persistent": false
    },
    "permissions": [
        "activeTab"
    ]
}